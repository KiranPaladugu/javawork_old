#This property realigns the IPT NMS alarms on Circuit Group modification, when set to true.
PlugIn.Async.CircuitGroup.Modify.RealignNMAlarms = false

#This property is used to create an alarm when TMF receives threshold exceed event from IPTNMS
PlugIn.em.emAlarmState = \
    alarm : { \
      notificationId '', \
      alarmIdentifier '', \
      objectName emsDn : { ems '' }, \
      nativeEMSName '', \
      objectType ot_ems, \
      emsTime '', \
      neTime '', \
      isClearable true, \
      perceivedSeverity ps_major, \
      assignedSeverity as_critical, \
      layerRate 1, \
      acknowledgeIndication ai_event_unacknowledged, \
      correlatedAlarmIdentifiers {}, \
      probableCause 'EMS_ALM_LOSS', \
      nativeProbableCause '', \
      probableCauseQualifier 'Network Alarm for ', \
      serviceAffecting sa_non_service_affecting, \
      affectedTPList {}, \
      additionalText '', \
      x733Info {}, \
      rcaiIndicator false \
  }


