#This property specifies if the user label of the ME shall be set to nativeEmsName or empty
PlugIn.objectCreation.ME.SetUserLabel=true

#For TR HN71406
#property specifies the format used to form the nativeEmsName of the ME
PlugIn.naming.ME.nativeEMSName.format = %1$s:%2$s:%3$s

#TR HO39044
#Property describes the format of RNName for an MLRA
PlugIn.naming.MLRA.RNName.format = %1$s,%2$s

#PlugIn.SBI.SNName=<Name>.<Short Name>.<Suffix>
PlugIn.SBI.SNName=

# Requires <EnableSecondaryAlarms = Yes> set in [EH] section of MV38.ini
PlugIn.SBI.CorrelatedAlarms=false
PlugIn.SBI.LinkAlarmsBufferSize=50
PlugIn.SBI.forceProcessingAlarm = false
# valid values: ResourceId, AlarmId, none
PlugIn.SBI.ProbableCauseQualifier = none
# valid values: ResourceId, AlarmId
PlugIn.SBI.EmsProbableCauseQualifier = AlarmId

# %1$s = NM name
# %2$d = EMId
# %3$d = NEId in EM
# %4$s = SNName longSNName
# %5$s = SNName shortSNName
# %6$s = SNName snSuffix OPTIONAL
PlugIn.SBI.NameFormat.ME = %1$s:%2$d:%3$d

PlugIn.Async.Host=${PlugIn.SBI.Host}
