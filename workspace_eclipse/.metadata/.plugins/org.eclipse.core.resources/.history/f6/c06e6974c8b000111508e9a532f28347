PlugIn.GCT.Mode=rexec
PlugIn.GCT.rexec.HostName=${PlugIn.SBI.Host}
PlugIn.GCT.rexec.Command=sh -c 'DISPLAY=@D /usr/bin/X11/xterm -ls < /dev/null > /dev/null 2>&1'
PlugIn-GCT.rexec.DestroyCmd =csh -c 'setenv DISPLAY @D; source /opt/mv38/MV38env.csh; /usr/bin/X11/xterm -e "$MV38/SCRIPT/MV38_kill HCI @U" < /dev/null > /dev/null'
PlugIn.GCT.CloseGUIAtSessionClosing =true
PlugIn-GCT.rexec.LaunchTimeout=120000
PlugIn-GCT.rexec.DestroyTimeout=120000
